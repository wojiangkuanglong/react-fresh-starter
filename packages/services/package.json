{
  "name": "@repo/services",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome check .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules .turbo",
    "api": "kubb generate",
    "check:apiunsafe": "biome check --fix --unsafe ./src"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@kubb/cli": "^4.15.2",
    "@kubb/core": "^4.15.2",
    "@kubb/plugin-client": "^4.15.2",
    "@kubb/plugin-faker": "^4.15.2",
    "@kubb/plugin-msw": "^4.15.2",
    "@kubb/plugin-oas": "^4.15.2",
    "@kubb/plugin-ts": "^4.15.2",
    "@kubb/plugin-zod": "^4.15.2",
    "@repo/typescript-config": "workspace:*",
    "msw": "catalog:",
    "rimraf": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "dependencies": {
    "@repo/lib": "workspace:*",
    "axios": "catalog:"
  },
  "exports": {
    "./api": {
      "require": "./dist/api.cjs",
      "import": "./dist/api.mjs"
    },
    "./faker": {
      "require": "./dist/faker.cjs",
      "import": "./dist/faker.mjs"
    },
    "./mock": {
      "require": "./dist/mock.cjs",
      "import": "./dist/mock.mjs"
    },
    "./model": {
      "require": "./dist/model.cjs",
      "import": "./dist/model.mjs"
    },
    "./zod": {
      "require": "./dist/zod.cjs",
      "import": "./dist/zod.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "api": [
        "./dist/api.d.ts"
      ],
      "model": [
        "./dist/model.d.ts"
      ],
      "zod": [
        "./dist/zod.d.ts"
      ],
      "mock": [
        "./dist/mock.d.ts"
      ],
      "faker": [
        "./dist/faker.d.ts"
      ]
    }
  }
}
