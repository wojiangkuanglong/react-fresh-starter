{
  "name": "react-fresh-starter",
  "description": "react项目启动模板",
  "private": false,
  "version": "1.0.0",
  "main": "src/app/index.tsx",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host 0.0.0.0",
    "start": "vite --host 0.0.0.0",
    "prebuild": "pnpm knip && rimraf dist && rimraf node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "pnpm build && vite preview",
    "format": "biome format --write src",
    "lint": "biome lint src --diagnostic-level=error",
    "check": "biome check --write src",
    "check:unsafe": "biome check --fix --unsafe src",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "taze": "pnpm dlx taze",
    "knip": "knip",
    "release": "standard-version && git push --follow-tags origin publish",
    "clean": "pnpm store prune && rimraf node_modules pnpm-lock.yaml dist",
    "scan": "pnpm dev & pnpm dlx react-scan@latest http://localhost:5173"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.9.0",
    "@tanstack/react-query-devtools": "^5.9.0",
    "@team/antd-config-provider": "^0.0.6",
    "antd": "^5.24.5",
    "axios": "^1.8.4",
    "click-to-react-component": "^1.1.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-i18next": "^15.4.1",
    "react-router": "^7.4.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/prompt-cli": "^19.8.0",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@kubb/cli": "^3.7.0",
    "@kubb/core": "^3.7.0",
    "@kubb/plugin-client": "^3.7.0",
    "@kubb/plugin-oas": "^3.7.0",
    "@kubb/plugin-react-query": "^3.7.0",
    "@kubb/plugin-ts": "^3.7.0",
    "@plugin-web-update-notification/vite": "^1.8.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/vite-react": "^3.4.0",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "knip": "^5.46.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.8.2",
    "unplugin-kubb": "^2.0.2",
    "vite": "^6.2.3",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
