{
  "name": "react-fresh-starter",
  "description": "react项目启动模板",
  "private": false,
  "version": "1.0.0",
  "main": "src/app/index.tsx",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host 0.0.0.0",
    "start": "pnpm dev",
    "prebuild": "pnpm knip && rimraf dist && rimraf node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "pnpm build && vite preview",
    "format": "biome format --write src",
    "lint": "biome lint src --diagnostic-level=error",
    "check": "biome check --write src",
    "check:unsafe": "biome check --fix --unsafe src",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "taze": "pnpm dlx taze",
    "knip": "knip",
    "release": "standard-version && git push --follow-tags origin publish",
    "clean": "pnpm store prune && rimraf node_modules pnpm-lock.yaml dist",
    "scan": "pnpm dev & pnpm dlx react-scan@latest http://localhost:5173",
    "api": "kubb generate && pnpm difit",
    "difit": "pnpm dlx difit .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "plop"
  },
  "engines": {
    "node": ">=22.12.0",
    "pnpm": ">=10.15.0"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.9.0",
    "@tanstack/react-query-devtools": "^5.9.0",
    "@tanstack/react-router": "^1.131.44",
    "@tanstack/react-router-devtools": "^1.131.44",
    "antd": "^5.27.3",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.7.3",
    "streamdown": "^1.3.0",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@kubb/cli": "^3.9.5",
    "@kubb/core": "^3.9.5",
    "@kubb/plugin-client": "^3.9.5",
    "@kubb/plugin-oas": "^3.9.5",
    "@kubb/plugin-redoc": "^3.18.3",
    "@kubb/plugin-ts": "^3.9.5",
    "@plugin-web-update-notification/vite": "^2.0.2",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@tanstack/router-plugin": "^1.131.44",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/vite-react": "^7.0.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.63.1",
    "lint-staged": "^16.1.6",
    "playwright": "^1.55.0",
    "plop": "^4.0.2",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "storybook": "^9.1.5",
    "tailwindcss": "^4.1.9",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
