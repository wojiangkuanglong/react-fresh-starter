{
  "name": "react-fresh-starter",
  "description": "react项目启动模板",
  "private": false,
  "version": "1.0.0",
  "main": "src/app/index.tsx",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host 0.0.0.0",
    "start": "vite --host 0.0.0.0",
    "prebuild": "pnpm knip && rimraf dist && rimraf node_modules/.vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "pnpm build && vite preview",
    "format": "biome format --write src",
    "lint": "biome lint src --diagnostic-level=error",
    "check": "biome check --write src",
    "check:unsafe": "biome check --fix --unsafe src",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "taze": "pnpm dlx taze",
    "knip": "knip",
    "release": "standard-version && git push --follow-tags origin publish",
    "clean": "pnpm store prune && rimraf node_modules pnpm-lock.yaml dist",
    "scan": "pnpm dev & pnpm dlx react-scan@latest http://localhost:5173",
    "api": "kubb generate"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "antd": "^5.27.1",
    "axios": "^1.11.0",
    "click-to-react-component": "^1.1.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "immer": "^10.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.7.2",
    "react-router": "^7.8.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@kubb/cli": "^3.18.1",
    "@kubb/core": "^3.18.1",
    "@kubb/plugin-client": "^3.18.1",
    "@kubb/plugin-oas": "^3.18.1",
    "@kubb/plugin-react-query": "^3.18.1",
    "@kubb/plugin-ts": "^3.18.1",
    "@plugin-web-update-notification/vite": "^2.0.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/vite-react": "^7.0.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "knip": "^5.63.0",
    "lint-staged": "^16.1.5",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
